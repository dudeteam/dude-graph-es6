  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dudeteam/dude-graph-es6.git"
  },
  "bugs": {
    "url": "https://github.com/dudeteam/dude-graph-es6/issues"
  },
  "homepage": "https://github.com/dudeteam/dude-graph-es6#readme",
  "dependencies": {
    "event-class-es6": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint-clang-formatter": "^1.3.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^11.4.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-stream": "^1.24.1",
    "sinon": "^4.1.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "jspm": {
    "main": "dude-graph",
    "format": "esm",
    "directories": {
      "lib": "src"
